<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Widget.BottomNavigationView"
        parent="Widget.Design.BottomNavigationView">
        <item name="fontFamily">@font/nanum_square_otfb</item>
        <item name="android:textSize">9sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:lineSpacingExtra">1sp</item>
    </style>

    <style name="CustomFullDialog">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="AnimationPopupStyle">
        <item name="android:windowEnterAnimation">@anim/open</item>
        <item name="android:windowExitAnimation">@anim/close</item>
    </style>

</resources>